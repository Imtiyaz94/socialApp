{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "quotes":["error","single", {"avoidEscape":true}],
        "import/extensions":["never",{
            "ignorePackages": true,"pattern": {
                "javascript": "ignorePackages"
                }
            }
        ]
    }
}
